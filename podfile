
# workspace文件名
workspace 'GPUIKit.xcworkspace'

# 本地工程
def _3rdPods
   
   pod 'FrameAccessor' , '~> 2.0'
   
end


target 'GPUIKit' do
  platform :ios, '9.0'
  project './GPUIKit/GPUIKit.xcodeproj'

end

# 本地工程
def localPods
    # UI库
    pod 'GPUIKit'          , :path => './GPUIKit/'
    
end

target 'GPUIKitDemo' do
  platform :ios, '9.0'
  project './GPUIKitDemo/GPUIKitDemo.xcodeproj'
  
  # loal
  localPods
  # 第三方库
  _3rdPods
end

